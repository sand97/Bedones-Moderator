{
  "name": "moderateur-bedones",
  "private": true,
  "scripts": {
    "generate": "prisma generate",
    "prisma-studio": "prisma studio",
    "db-seed": "prisma db seed",
    "db-reset": "prisma migrate dev reset",
    "dx:next": "run-s migrate-dev db-seed && next dev",
    "dx:prisma-studio": "pnpm prisma-studio",
    "dx": "run-p dx:* --print-label",
    "dev": "pnpm dx:next",
    "prebuild": "pnpm generate",
    "build": "next build",
    "start": "next start",
    "lint": "eslint --cache src",
    "lint-fix": "pnpm lint --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "migrate-dev": "prisma migrate dev",
    "migrate": "prisma migrate deploy",
    "db:push": "prisma db push",
    "test-unit": "vitest",
    "test-e2e": "playwright test",
    "test-start": "run-s test-unit test-e2e",
    "postinstall": "pnpm generate",
    "typecheck": "tsc --noEmit",
    "docker:build": "docker build -t moderateur-bedones .",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f app"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "prettier": {
    "printWidth": 80,
    "trailingComma": "all",
    "singleQuote": true
  },
